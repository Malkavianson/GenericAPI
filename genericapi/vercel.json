{
	"installCommand": "yarn && yarn format && yarn lint",
	"buildCommand": "nest build",
	"outputDirectory": "dist",
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
		}
	]
}
