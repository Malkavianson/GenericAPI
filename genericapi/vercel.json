{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"headers": [
		{
			"source": "/",
			"headers": [
				{
					"key": "Access-Control-Allow-*",
					"value": "true"
				}
			]
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
		}
	]
}
